</section>

<script>

var SLIDE_SPEED_MS = 200; // 折叠面板动画所需时间
var zoom = $(window).width() / $("body").width(); // html 的zoom值

/* 父容器被点击 */
var $parent = $('.acc-parent');
$parent.click(function() {
$closeTarget = $('.acc-parent.open').not($(this));
toggleMyIcon($closeTarget, true);
toggleMyChild($closeTarget.next('.acc-child'), true);
var $this = $(this);
var isMyChildOpen = $this.hasClass('open');
toggleMyIcon($this, isMyChildOpen);
toggleMyChild($this.next('.acc-child'), isMyChildOpen);
});

function toggleMyIcon($this, isMyChildOpen) {
$this
.toggleClass('open', !isMyChildOpen)
.toggleClass('closed', isMyChildOpen);
}

function toggleMyChild($myChild, isMyChildOpen) {
// 关闭/打开自己的子容器
if(isMyChildOpen) {
$myChild.slideUp(SLIDE_SPEED_MS);
}
else {
$myChild.slideDown(SLIDE_SPEED_MS);
}
}

</script>
</div>
</body>
</html>
